Функция Оформил(ОформилСтруктура)
		
	РезультатФункции = Новый Структура;
	
	РезультатФункции.Вставить("Фамилия",  ОформилСтруктура.Фамилия);
	РезультатФункции.Вставить("Имя",      ОформилСтруктура.Имя);
	РезультатФункции.Вставить("Отчество", ОформилСтруктура.Отчество);
	
	МассивТелефонов = Новый Массив;
	ТелефоныОформителя = ЗначениеИзСтрокиВнутр(ОформилСтруктура.Телефоны);
	Для Каждого Телефон Из ТелефоныОформителя Цикл
		СтруктураТелефон = Новый Структура;
		СтруктураТелефон.Вставить("Значение", Телефон);
		МассивТелефонов.Добавить(СтруктураТелефон);
	КонецЦикла;
	
	МассивЭлектроннойПочты = Новый Массив;
	Если ЗначениеЗаполнено(ОформилСтруктура.ЭлектроннаяПочта) Тогда		
		СтруктураЭлектроннойПочты = Новый Структура("Значение", ОформилСтруктура.ЭлектроннаяПочта[0]);		
		МассивЭлектроннойПочты.Добавить(СтруктураЭлектроннойПочты);			
	КонецЕсли;
	
	Контакты = Новый Структура;
	Контакты.Вставить("Телефон",          МассивТелефонов);
	Контакты.Вставить("ЭлектроннаяПочта", МассивЭлектроннойПочты);
	
	РезультатФункции.Вставить("Контакты", Контакты);
	
	Возврат РезультатФункции;
	
КонецФункции

Функция ДанныеСоставителя(ДанныеДокумента)
	
	РезультатФункции = Новый Структура;
	
	// Заполним реквизиты. Используется для расчета данных по нашей организации	
	Реквизиты = МодульКода("Saby_ТНВыгрузкаСервер").ДанныеЮрЛица("Оформитель", ДанныеДокумента);
		
	РезультатФункции.Вставить("НазваниеПолное", Реквизиты.Название.Полное);
	РезультатФункции.Вставить("ДанныеЮрЛица",   Реквизиты);
	
	Основание = ДанныеДокумента.Составитель_НаОснованииДокумент;
	Если ЗначениеЗаполнено(Основание) Тогда
		
		ДанныеДок = ПодстановкиОснованияСоставителя(ДанныеДокумента, Основание);
		
		РезультатФункции.Вставить("Основание", Новый Структура);
		РезультатФункции.Основание.Вставить("Документ", ДанныеДок);
		
	КонецЕсли;
	
	Возврат РезультатФункции;
	
КонецФункции

Функция ПодстановкиОснованияСоставителя(ДанныеДокумента, ОснованиеСоставителя)
	
	СтруктураДокументаЭПД = МодульКода("Saby_ТНОбщегоНазначенияКлиентСервер").СтруктураДокументаЭПД();
	
	Если ДанныеДокумента.ЭтоВО Тогда 
		
		Отбор = Новый Структура;
		Отбор.Вставить("Тип", "СоставительНаОсновании"); 
		
		НайденныеСтроки = МодульКода("Saby_ТНОбщегоНазначенияКлиентСервер").НайтиСтрокиУниверсально(
			ДанныеДокумента.ДокументыЭПД, Отбор);
		Если НайденныеСтроки.Количество() > 0 Тогда
			СтруктураДокументаЭПД = НайденныеСтроки[0];
		КонецЕсли;
		
	Иначе
		
		МассивДокументовЭПД = Новый Массив;
		СтруктураДокументаЭПД.Ссылка = ОснованиеСоставителя;
		МассивДокументовЭПД.Добавить(СтруктураДокументаЭПД);
		
		МодульКода("Saby_ТНВыгрузкаСервер").ЗаполнитьДанныеДокументовЭПД(МассивДокументовЭПД);
		
	КонецЕсли;
	
	Возврат МодульКода("Saby_ТНВыгрузкаСервер").ПодстановкаДокумента(СтруктураДокументаЭПД);
	
КонецФункции

Функция ПодстановкиИныхПолучателей(ДанныеДокумента)
	
	РезультатФункции = Новый Массив;
	
	РольИнойПолучатель = ЗначениеМетаданных("Saby_РолиКонтрагентов.ИнойПолучатель");
	
	Для Каждого СтруктураЮрЛица Из ДанныеДокумента.ДанныеЮрЛиц Цикл
		
		Если СтруктураЮрЛица.Роль <> РольИнойПолучатель Тогда
			Продолжить;
		КонецЕсли;
		
		ПодстановкаИногоПолучателя = МодульКода("Saby_ТНВыгрузкаСервер").ДанныеЮрЛица(
			"Отправитель", ДанныеДокумента, , СтруктураЮрЛица);
		
		РезультатФункции.Добавить(ПодстановкаИногоПолучателя);
		
	КонецЦикла;
	
	Возврат РезультатФункции;
	
КонецФункции

